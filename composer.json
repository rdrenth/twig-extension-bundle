{
    "name": "rdrenth/twig-extension-bundle",
    "description": "Symfony bundle with useful twig extensions",
    "keywords": ["rdrenth", "symfony", "bundle", "twig", "twig extension", "stringy"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/rdrenth/twig-extension-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronald Drenth",
            "email": "ronalddrenth@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3.3|^7.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "twig/twig": "~1.20|~2.0",
        "danielstjules/stringy": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": { "Rdrenth\\Bundle\\TwigExtensionBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
